fun pow(a: Number, b: Number) -> Number {
    res = 1
    while (b > 0) {
        if (b % 2 == 1)
            res *= a
        a *= a
        b /= 2
    }
    return res
}

fun sqrt(n: Fixed) -> Fixed {
    const e = 0.001
    x = n
    y = 1.0
    while (x - y > e) {
        x = (x + y) / 2.0
        y = n / x
    }
    return x
}

fun fac(x: Number) -> Number {
    ret = 1
    while (x > 1) {
        ret *= x
        --x
    }
    return ret
}

run for @a {
    a = 5
    b = 3
    c = 5.0
    print(a, "^", b, " = ", pow(5,3))
    print(a, "! = ", fac(5))
    print("√9 = ", sqrt(9.0))
    print("√5 = ", sqrt(c))
    print("√25 = ", sqrt(25.0))
}