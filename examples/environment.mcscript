fun onTick() {
    run for @a {
        light_level = getLightLevel()
        biome = getBiome()

        weather = "Clear Weather"
        if (isThundering())
            weather = "Thunder"
        else if (isRaining())
            weather = "Rain"

        actionbar("{}, biomeId: {}, light level: {}", weather, biome, light_level)
    }
}
